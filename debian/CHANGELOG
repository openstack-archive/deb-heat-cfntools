    {
        "AWS::CloudFormation::Init": {
            "config": {
                "packages": {
                    "yum": {
                        "rubygem-fattr" : []
                    }
                },
                "services": {
                    "sysvinit": {
                        "httpd": {
                            "enabled" : "true",
                            "ensureRunning" : "true"
                        }
                    }
                }
            }
        }
    }

